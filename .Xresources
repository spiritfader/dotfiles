! cursor
Xcursor.theme:                Adwaita
Xcursor.size:                 12

! xft
xft.antialias:                true

! generic settings
!*foreground:                  #cccccc
!*background:                  #000000

! XTerm settings
XTerm.termName: xterm-256color
! force utf-8
XTerm.vt100.locale: false
XTerm.vt100.utf8: true
! make 'Alt' key behave as on other terminal emulators
XTerm.vt100.metaSendsEscape: true
! fix the backspace key
XTerm.vt100.backarrowKey: false
XTerm.ttyModes: erase ^?
XTerm.vt100.saveLines: 8192
XTerm.vt100.faceName: Terminus:size=12:antialias=true
XTerm.vt100.bellIsUrgent: true
XTerm*vt100*transparent: true
XTerm*vt100*allowTitleOps: true
XTerm.vt100.selectToClipboard: true

! xterm baclkarch
!XTerm*VT100*geometry:         80x24
!XTerm*faceName:               xft:xos4 Terminus:size=12:antialias=true
!XTerm*faceSize:               12
!XTerm*renderFont:             true
!XTerm*utf8Title:              true
!XTerm*trimSelection:          true
!XTerm*cutNewline:             false
!XTerm*highlightSelection:     true

! URXVT settings
URxvt*buffered: true
URxvt*geometry: 80x24
URxvt*background: [90]Black
URxvt*foreground: White
!URxvt*underlineColor:ting o get
!URxvt*highlightColor:
!URxvt*highlightTextColor:
!URxvt*cursorColor:
!URxvt*cursorColor2:
!URxvt*reverseVideo:
URxvt*jumpScroll: false
URxvt*skipScroll: false
!URxvt*fading: 80
!URxvt*fadeColor: Magenta
!URxvt*iconFile:
!URxvt*scrollColor:
!URxvt*troughColor:
!URxvt*borderColor:
URxvt*intensityStyles: true 
URxvt*title: urxvt
!URxvt*iconName:
URxvt*mapAlert: false
URxvt*urgentOnBell: true
URxvt*visualBell: true
URxvt*loginShell: true
URxvt*utmpInhibit: false
!URxvt*print-pipe:
URxvt*scrollstyle: plain
!URxvt*thickness: 4
URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt*scrollBar_floating: true
URxvt*scrollBar_align: centre
URxvt*scrollTtyOutput: true
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
URxvt*saveLines: 1000
URxvt*internalBorder: 1
URxvt*externalBorder: 1
URxvt*borderLess: false
URxvt*skipBuiltInGlyphs: false
!URxvt*termName: urxvt
URxvt*lineSpace: 0
!URxvt*meta8: false
URxvt*mouseWheelScrollPage: true
URxvt*pastableTabs: false
